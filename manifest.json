{
 "manifest_version": 2,
 "name": "Get Shit Done",
 "description": "Todo app as chrome extension",
 "version": "1.0",
 "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
 "browser_action": {
 "default_icon": "icon.png",
 "default_popup": "index.html"
 },
 "permissions": ["contextMenus"]
}